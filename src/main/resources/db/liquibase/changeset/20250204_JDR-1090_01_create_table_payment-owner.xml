<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.8.xsd">

    <changeSet id="JDR-1090_01" author="sina@paraw.top">
        <createTable tableName="payment_owner">
            <column name="id" type="numeric(38, 0)">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="_created_date" type="timestamp"/>
            <column name="_updated_date" type="timestamp"/>
            <column name="_version" type="integer"/>
            <column name="_description" type="text"/>
            <column name="_identifier" type="text">
                <constraints unique="true"/>
            </column>
            <column name="_name" type="text"/>
            <column name="configs" type="jsonb"/>
            <column name="self_config" type="jsonb"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
